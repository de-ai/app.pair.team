{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": false,
      "keepProfileChanges": false,
      "profile": "${workspaceFolder}",
      "name": "Launch Firefox on localhost:3000",
      "url": "http://localhost:3000/team#login",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "moz-extension://0fe61bde-c36b-8944-956f-5dacb1f1f855/inject",
          "path": "${workspaceFolder}/src/redux/middleware"
        }
      ]
    },
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "profileDir": "/Users/mattholcombe/Library/Application Support/Firefox/Profiles/1spydc8c.default-debug",
      "name": "Launch Firefox on 192.168.1.50:3000",
      "url": "http://192.168.1.50:3000/team#login",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "moz-extension://2dbba05c-0889-a44a-afef-ef7ac8ae2e55/inject",
          "path": "${workspaceFolder}/src/redux/middleware"
        },
        {
          "url": "moz-extension://2dbba05c-0889-a44a-afef-ef7ac8ae2e55/inject",
          "path": "${workspaceFolder}/src/redux/middleware"
        },
        {
          "url": "moz-extension://0fe61bde-c36b-8944-956f-5dacb1f1f855/inject",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/team/#login",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "skipFiles": [
        "node_modules/**/*.js"
      ]
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "urlFilter": "http://localhost:3000/team/*",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*",
        "/Volumes/Labor/projects/sites/pairurl.com/*": "${workspaceRoot}/*"
      },
      "trace": true,
      "sourceMaps": true
    }
  ]
}
