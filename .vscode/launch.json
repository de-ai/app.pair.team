{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch Firefox on localhost:3000",
      "url": "http://localhost:3000/app/pairurl-com/pairurl-com/390/desktop-macos/views/16089/comments/16911",
      "webRoot": "${workspaceFolder}",
      "disableOptimisticBPs": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Debug Inspector on 5858",
      "program": "${workspaceRoot}/src/index.js",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/react-scripts",
      "args": [
        "start",
        "--inspect-brk=5858",
        "--no-cache"
      ],
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "userDataDir": true,
      "disableOptimisticBPs": false,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "skipFiles": [
        "node_modules/**/*.js"
      ]
    },
    {
      "name": "Debug Tests",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/react-scripts",
      "args": [
        "start",
        "--inspect-brk=5858",
        "--runInBand",
        "--no-cache"
      ],
      "disableOptimisticBPs": false,
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Debug CRA Tests",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/react-scripts",
      "args": [
        "start",
        "--runInBand",
        "--inspect-brk=5858"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "CI": "true"
      },
      "disableOptimisticBPs": true
    }
  ]
}