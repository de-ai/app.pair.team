{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via Yarn",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["start:debug"],
      "port": 5858
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch VS Code (Web, Chrome)",
      "url": "http://localhost:3000/app/pairurl-com/pairurl-com/204/558/views",
      "port": 9222,
      "timeout": 20000,
      "env": {
        "VSCODE_EXTHOST_WILL_SEND_SOCKET": null
      },
      "breakOnLoad": false,
      "runtimeArgs": [
        "--inspect=5875",
        "--no-cached-data",
      ],
      "webRoot": "${workspaceFolder}",
      "userDataDir": true,
      "skipFiles": [
         "<node_internals>/**/*.js",
         "${workspaceFolder}/node_modules/**/*.js",
         "${workspaceFolder}/vendor/**/*.js",
         "${workspaceFolder}/build/**/*.js",
      ]
    }
  ]
}